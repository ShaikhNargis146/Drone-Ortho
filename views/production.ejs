            console.log("%c ---------------------------------------", 'background: #222; color: #bada55');
        };
    </script>
    <% _.each(jsFiles,function(file) {  %>
        <script src="frontend/js/<%= file %>"></script>
        <% }); %>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGSUwk7KdhoDlwzq7CSpeJDcOjKzu-xRA&sensor=false&libraries=geometry,drawing,places&callback=initMap"
